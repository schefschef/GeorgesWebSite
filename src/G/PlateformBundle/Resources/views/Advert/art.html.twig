{% extends "GPlateformBundle::layout.html.twig" %}


{% block title %}

{% endblock %}

{% block g_plateform_body %}
<div class="center">
    <div class="im1">
        <img  width="500" src="{{ asset('img/two.jpg') }}">
    </div>
    <div class="im2">
        <img  width="500" src="{{ asset('img/one.jpg') }}">
    </div>
    <div class="im3">
        <img  width="500" src="{{ asset('img/three.jpg') }}">
    </div>
    <div class="im4">
        <img  width="500" src="{{ asset('img/coverGeorges.jpg') }}">
    </div>
</div>
    <script>
        var options = [ {selector: '.im1', offset: 10, callback: function(el) { Materialize.fadeInImage($(el));}},
            {selector: '.im2', offset: 10, callback: function(el) { Materialize.fadeInImage($(el));}},
                {selector: '.im3',offset: 10, callback: function(el) { Materialize.fadeInImage($(el));}},
            {selector: '.im4',offset: 10, callback: function(el) { Materialize.fadeInImage($(el));}}];

        Materialize.scrollFire(options);

    </script>
{% endblock %}
